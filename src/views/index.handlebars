<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{{titulo}}</title>
  </head>
  <body>
    <main class="row justify-content-center">
      <form class="mt-2 col-9 col-md-4 align-items-sm-center" id="form">
        <div class="mb-3">
          <label for="exampleInputEmail1" class="form-label fs-5">NOMBRE DE
            USUARIO</label>
          <input
            type="text"
            class="form-control"
            id="nombre"
            aria-describedby="emailHelp"
          />
        </div>
        <div class="mb-3">
          <label
            for="exampleInputEmail1"
            class="form-label fs-5"
          >MENSAJE</label>
          <textarea
            class="form-control"
            name=""
            id="textArea"
            cols="30"
            rows="10"
          ></textarea>
        </div>
        <button
          type="submit"
          class="btn btn-primary align-items-center"
          id="enviar"
          value="enviar"
          name="enviar"
        >ENVIAR</button>
      </form>
    </main>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.6.1/socket.io.min.js"
      integrity="sha512-AI5A3zIoeRSEEX9z3Vyir8NqSMC1pY7r5h2cE+9J6FLsoEmSSGLFaqMQw8SWvoONXogkfFrkQiJfLeHLz3+HOg=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    ></script>
    <script src="js/index.js"></script>
  </body>
</html>